block variables
  - keywords = 'frontend'
  - description = 'junior frontend разработчика'

doctype html
html(lang='en')
    head
      meta(charset="utf-8")
      meta(name="keywords" content=keywords)
      meta(name="description" content=description)
      title=pageTitle
      link(rel="icon" href="favicon.svg" type="image/svg+xml" sizes="16x16 32x32")
      link(rel="icon" href="favicon.png" type="image/png" sizes="16x16 32x32")
      meta(name='viewport' content='width=device-width, initial-scale=1.0')
    body
      include ./example.pug
